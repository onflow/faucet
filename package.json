{
  "name": "faucet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "db-migrate-dev": "prisma migrate dev --preview-feature",
    "db-migrate-deploy": "prisma migrate deploy --preview-feature",
    "db-seed": "node prisma/seed.mjs",
    "deploy": "npm run build && npm run db-migrate-deploy && npm run db-seed",
    "dev-deploy-contracts": "flow project deploy --network=emulator --update",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "tsc": "./node_modules/typescript/bin/tsc",
    "check": "next lint && npm run tsc",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.11.0",
    "@onflow/fcl": "^1.13.3",
    "@prisma/client": "^6.1.0",
    "@theme-ui/match-media": "^0.17.1",
    "@theme-ui/mdx": "^0.17.1",
    "elliptic": "^6.6.1",
    "formik": "^2.4.6",
    "hcaptcha": "^0.2.0",
    "mixpanel-browser": "^2.58.0",
    "next": "15.1.2",
    "nextjs-google-analytics": "^2.3.7",
    "react": "^19.0.0",
    "react-aria": "^3.36.0",
    "react-dom": "^19.0.0",
    "theme-ui": "^0.17.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/mixpanel-browser": "^2.50.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "postcss": "^8",
    "prisma": "^6.1.0",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
