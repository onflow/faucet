{
  "name": "flow-faucet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "db-migrate-dev": "prisma migrate dev --preview-feature",
    "db-migrate-deploy": "prisma migrate deploy --preview-feature",
