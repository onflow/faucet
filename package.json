    "@theme-ui/match-media": "^0.9.1",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^2.11.1",
    "elliptic": "^6.5.4",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.2",
    "hcaptcha": "0.0.2",
    "mixpanel-browser": "^2.41.0",
    "next": "^12.2.5",
    "nextjs-google-analytics": "^2.3.0",
